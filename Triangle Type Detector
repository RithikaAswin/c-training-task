#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int a,b,c;
    scanf("%d %d %d",&a,&b,&c);
    if(a==b&&b==c){
        printf("Equilateral Triangle");
    }
    else if(a==b||a==c||a==b){
        printf("Isosceles Triangle");
    }
    else if(a + b > c&&b + c > a&&c + a > b){
        printf("Scalene Triangle");
    }
    else{
        printf("Not a valid triangle");
    }
    return 0;
}
