#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int main(){
    int a,b,sum,diff,pro,quo,rem;
    scanf("%d%d",&a,&b);
    sum=a+b;
    diff=a-b;
    pro=a*b;
    quo=a/b;
    rem=a%b;
    printf("Sum: %d\n",sum);
    printf("Difference: %d\n",diff);
    printf("Product: %d\n",pro);
    printf("Quotient: %d\n",quo);
    printf("Remainder: %d\n",rem);
}
