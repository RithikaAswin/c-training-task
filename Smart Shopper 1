#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int c1,c2,c3,i1,i2,i3,bug;
    float sum,per;
    scanf("%d %d",&c1,&i1);
    scanf("%d %d",&c2,&i2);
    scanf("%d %d",&c3,&i3);
    scanf("%d",&bug);
    sum=(c1*i1)+(c2*i2)+(c3*i3);
    per=sum+sum*0.1;
    printf("Total (Before Tax): %.2f\n",sum);
    printf("Total (After Tax): %.2f\n",per);
    if(bug<=per){
        printf("Over Budget");
    }
    else{
        printf("Within Budget");
    }
    return 0;
}
